{
  "version": 2,
  "builds": [
    {
      "src": "api/record.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/trigger-github.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/record",
      "dest": "/api/record.js"
    },
    {
      "src": "/api/trigger-github",
      "dest": "/api/trigger-github.js"
    }
  ]
}
